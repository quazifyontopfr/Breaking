local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Patskorn/GUI/main/Copy-SynapOver.lua"))()

local GUI = library:new("Breaking Story","[ RightControl ]")
local Tab1 = GUI:Tap("Main")
local Tab2 = GUI:Tap("Role")
local Tab3 = GUI:Tap("Informations")

Tab2:Button("Bat",function()
    local args = {
    [1] = "Bat",
    [2] = false,
    [3] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MakeRole"):FireServer(unpack(args))
end)

Tab2:Button("Medkit",function()
    local args = {
